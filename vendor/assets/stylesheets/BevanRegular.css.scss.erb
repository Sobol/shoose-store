@font-face {
  font-family: 'BevanRegular';
  src: url("<%= "//#{AppConfig.host.to_s}/assets/Bevan-webfont.eot" %>");
  src: url("<%= "//#{AppConfig.host.to_s}/assets/Bevan-webfont.eot?#iefix" %>") format('embedded-opentype'),
    url("<%= "//#{AppConfig.host.to_s}/assets/Bevan-webfont.woff" %>") format('woff'),
    url("<%= "//#{AppConfig.host.to_s}/assets/Bevan-webfont.ttf" %>") format('truetype'),
    url("<%= "//#{AppConfig.host.to_s}/assets/Bevan-webfont.svg#BevanRegular" %>") format('svg');
  font-weight: normal;
  font-style: normal;
}
